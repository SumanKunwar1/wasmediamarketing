        </ol>
      </section>

      <!-- Main content -->
      <section class="content">
        <% if (faq && faq.content && faq.content.length> 0) { %>
          <% faq.content.forEach(function(section) { %>
            <h1>
              <%= section.sectionTitle || 'Frequently Asked Questions' %>
            </h1>
            <p>
              <%= section.Description %>
            </p>
            <!-- Main row -->
            <div class="row">
              <!-- Left col -->
              <section class="col-md-12">
                <div class="col-lg-12">
                  <div class="box">
                    <div class="box-body">
                      <table id="example" class="table table-bordered" cellspacing="0" width="100%">
                        <thead>
                          <tr>
                            <th>SN</th>
                            <th>Question</th>
                            <th>Answer</th>
                            <th>Action</th>
                          </tr>
                        </thead>
                        <tfoot>
                          <tr>
                            <th>SN</th>
                            <th>Question</th>
                            <th>Answer</th>
                            <th>Action</th>
                          </tr>
                        </tfoot>
                        <tbody>
                          <% section.tabs.forEach(function(tab, index) { %>
                            <tr>
                              <td>
                                <%= index + 1 %>
                              </td>
                              <td>
                                <%= tab.question %>
                              </td>
                              <td>
                                <%= tab.answer %>
                              </td>
                              <td>
                                <button class="btn btn-danger btn-sm"
                                  onclick="deletefaq('<%= tab._id %>')">Delete</button>
                              </td>
                            </tr>
                            <% }); %>
                        </tbody>
                      </table>
                    </div><!-- /.box-body -->
                  </div>
                </div>
              </section>
            </div>
            <% }); %>
              <% } else { %>
                <!-- If no FAQ content is present -->
                <p>No FAQ data available.</p>
                <% } %>
      </section><!-- /.content -->
      <!-- /.content-wrapper -->

