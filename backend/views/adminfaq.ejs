    <script type="text/javascript">
      $(document).ready(function () {
        $('#example').DataTable({
          "lengthMenu": [[50, 100, -1], [50, 100, "All"]],
        });
      });
    </script>
    <script>
      async function deletefaq(id) {
        const confirmDelete = confirm("Are you sure you want to delete this FAQ entry?");

        if (confirmDelete) {
          try {
            // Perform the DELETE request to the server
            const response = await fetch(`/admin/faq/${id}`, {
              method: 'DELETE',
              headers: {
                'Content-Type': 'application/json',
                'X-CSRF-TOKEN': document.querySelector('meta[name="csrf-token"]').getAttribute('content'), // CSRF token from the meta tag
              }
            });

            if (response.ok) {
              const data = await response.json();
              alert('FAQ deleted successfully!');

              window.location.reload();
            } else {
              const errorData = await response.json();
              alert('Error deleting FAQ: ' + errorData.message);
            }
          } catch (error) {
            console.error('Error:', error);
            alert('An error occurred while deleting the FAQ.');
          }
        }
      }
    </script>







