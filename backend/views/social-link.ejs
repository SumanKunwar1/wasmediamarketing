  <script>
    document.addEventListener('DOMContentLoaded', () => {
      // Handle form submission
      const form = document.getElementById('social-link-form');
      form.addEventListener('submit', async function (e) {
        e.preventDefault();  // Prevent default form submission

        // Collect the form data
        const formData = {
          name: document.querySelector('input[name="name"]').value,
          phoneNumber: document.querySelector('input[name="phoneNumber"]').value,
          location: document.querySelector('input[name="location"]').value,
          email: document.querySelector('input[name="email"]').value,
          twitter: document.querySelector('input[name="twitter"]').value,
          facebook: document.querySelector('input[name="facebook"]').value,
          google_plus: document.querySelector('input[name="google_plus"]').value,
          youtube: document.querySelector('input[name="youtube"]').value,
          linkedin: document.querySelector('input[name="linkedin"]').value,
          instagram: document.querySelector('input[name="instagram"]').value,
        };
        console.log(formData);
        // Send data via fetch
        try {
          const response = await fetch('/admin/info', {
            method: 'POST',
            headers: {
              'Content-Type': 'application/json',
            },
            body: JSON.stringify(formData),  // Convert form data to JSON string
          });

          // Check for successful response
          if (response.ok) {
            const result = await response.json();
            alert('Information updated successfully!');
            console.log(result);
          } else {
            throw new Error('Error updating information.');
          }
        } catch (error) {
          alert('Error updating information.');
          console.error(error);
        }
      });
    });
  </script>






