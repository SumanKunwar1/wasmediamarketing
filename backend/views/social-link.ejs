                    </div>
                  </div>

                  <!-- Social Media Links Section -->
                  <div class="form-group">
                    <label for="inputTwitter" class="col-sm-2 control-label">Twitter</label>
                    <div class="col-sm-9">
                      <input type="text" class="form-control" value="<%= info.length > 0 ? info[0].twitter : '' %>"
