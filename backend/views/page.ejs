          </li>
          <li class="">
            <a href="/admin/social-link">
              <i class="fa fa-link"></i> <span>Social Links</span>
            </a>
          </li>
          <li class="">
            <a href="/admin/navbar">
              <i class="fa fa-cog"></i> <span>Navbar</span>
            </a>
          </li>
          <li class="">
            <a href="/admin/subscriber">
              <i class="fa fa-cog"></i> <span>NewsLetter</span>
            </a>
          </li>

        </ul>
      </section>
      <!-- /.sidebar -->
    </aside>



    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">

      <!-- Content Header (Page header) -->
      <section class="content-header">
        <h1>
          <div class="col-md-9">
            Pages
            <small>The description of pages can be changed here. </small>
          </div>
          <div class="col-md-3">
            <a href="/admin/addpage" class="btn btn-primary">Add Page</a>
          </div>
        </h1>
        <ol class="breadcrumb">
          <li><a href="/admin/dashboard"><i class="fa fa-dashboard"></i> Home</a></li>
          <li class="active"> Pages</li>
        </ol>
      </section>

      <!-- Main content -->
      <section class="content">
        <div class="row">
          <section class="col-md-12">
            <div class="col-lg-12">
              <div class="box">
                <div class="box-body">
                  <table id="example" class="table table-bordered" cellspacing="0" width="100%">
                    <thead>
                      <tr>
                        <th>SN</th>
                        <th>Title</th>
                        <th>URL Slug</th>
                        <th>Is Published</th>
                        <th>Created At</th>
                        <th>Updated At</th>
                        <th>Meta Description</th>
                        <th>Page Type</th> <!-- New Column -->
                        <th>Action</th>
                      </tr>
                    </thead>
                    <tfoot>
                      <tr>
                        <th>SN</th>
                        <th>Title</th>
                        <th>URL Slug</th>
                        <th>Is Published</th>
                        <th>Created At</th>
                        <th>Updated At</th>
                        <th>Meta Description</th>
                        <th>Page Type</th> <!-- New Column -->
                        <th>Action</th>
                      </tr>
                    </tfoot>
                    <tbody>
                      <% pages.forEach((page, index)=> { %>
                        <tr>
                          <td>
                            <%= index + 1 %>
                          </td>
                          <td>
                            <%= page.title %>
                          </td>
                          <td><a href="/<%= page.urlSlug %>">
                              <%= page.urlSlug %>
                            </a></td>
                          <td>
                            <%= page.isPublished ? 'Yes' : 'No' %>
                          </td>
                          <td>
                            <%= page.createdAt %>
                          </td>
                          <td>
                            <%= page.updatedAt %>
                          </td>
                          <td>
                            <%= page.metaDescription %>
                          </td>
                          <td>
                            <% let pageType='Subpage' ; if (!page.parentId && page.order===0) { pageType='Main Page' ; }
                              else if (!page.parentId && page.order===1) { pageType='Footer Page' ; } %>
                              <%= pageType %>
                          </td>
                          <td>
                            <a href="/admin/page/edit/<%= page._id %>">
                              <button class="btn btn-primary"><i class="fa fa-eye"></i></button>
                            </a>
                            <form method="POST" action="/admin/pages/<%= page._id %>" style="display:inline;">
                              <button type="submit" class="btn btn-danger"
                                onclick="return confirm('Are you sure you want to delete?');"><i
                                  class="fa fa-trash"></i></button>
                            </form>
                          </td>
                        </tr>
                        <% }); %>
                    </tbody>
                  </table>
                </div><!-- /.box-body -->
              </div>
            </div>
          </section>
        </div>
      </section><!-- /.content -->
    </div><!-- /.content-wrapper -->

    <!-- footer -->
    <footer class="main-footer">

      <strong>Copyright &copy;

        <strong>
    </footer>
  </div><!-- ./wrapper -->
  <script data-cfasync="false" src="/cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script>
